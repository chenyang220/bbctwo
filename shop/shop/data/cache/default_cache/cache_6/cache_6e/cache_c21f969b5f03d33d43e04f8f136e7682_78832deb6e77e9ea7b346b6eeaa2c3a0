            // 谁有active就给谁默认词
            $(function () {
                if ($("#shop").is(".active")) {
                    $("#site_keywords").attr("placeholder", "");
                } else {
                    $("#site_keywords").attr("placeholder", "");
                }
            });
            
            // 当点击宝贝时，填充商品关键词
            function searchWords() {
                $("#site_keywords").val("");
                $("#site_keywords").attr("placeholder", "");
            }
            
            // 当点击店铺时，填充店铺关键词
            function searchShopWords() {
                $("#site_keywords").attr("placeholder", "");
            }
        
