<script>
$(function() {
	if(!$.cookie("c_ip")){
		setTimeout("setClientIp()",3000);//设置5秒钟后执行
	}	
});
function setClientIp(){
	var ip = returnCitySN["cip"];
	$.ajax({
		url:SITE_URL + '?ctl=Index&met=setClientIpByCookie',
		type:'POST',
		data:{ip:ip},
		success: function (data) {
			$.cookie('c_ip',ip);
		}
	});
}
</script>
